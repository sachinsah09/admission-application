<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.1.xsd">

  <grid name="college-grid" title="College-Grid"
    model="com.axelor.admission.db.College">
    <field name="name" />
    <field name="facultyEntryList" />
  </grid>

  <form name="college-form" title="College Form"
    model="com.axelor.admission.db.College">
    <panel title="Fill all Details">
      <field name="name" />
    </panel>
    <panel-related field="facultyEntryList"
      title="Faculty Entry" />
  </form>
</object-views>
