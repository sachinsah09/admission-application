<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.1.xsd">

  <grid name="college-entry-grid" title="College-Entry-Grid"
    model="com.axelor.admission.db.CollegeEntry">
    <field name="college" />
    <field name="sequence" />
  </grid>

  <form name="college-entry-form" title="College-Entry-Form"
    model="com.axelor.admission.db.CollegeEntry">
    <panel title="Fill all Details">
      <field name="college" />
      <field name="admissionEntry" hidden="true" />
      <field name="sequence" />
    </panel>
  </form>
</object-views>
